    "lint",
    "test"
  ],
  "dependencies": {
    "allure-commandline": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "browserstack-local": "^1.3.0",
    "copy": "^0.3.1",
    "cross-env": "^5.0.5",
    "deepmerge": "^1.3.2",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "es6-plato": "^1.0.14",
    "eslint": "^3.19.0",
    "eslint-config-llbean": "^1.3.1",
    "eslint-plugin-no-unused-expressions": "^0.1.0",
    "eslint-plugin-webdriverio": "^1.0.1",
    "it-each": "^0.3.1",
    "mocha": "^3.3.0",
    "pre-commit": "^1.2.2",
    "request": "^2.83.0",
    "run-sequence": "^1.2.2",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-browserstack-service": "^0.1.4",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-selenium-standalone-service": "0.0.8",
    "wdio-spec-reporter": "^0.1.0",
    "web-automation-base": "^0.1.0",
    "web-automation-category-page": "^0.1.0",
    "web-automation-footer": "^0.1.1",
    "web-automation-header": "latest",
    "web-automation-selectors": "^0.1.1",
    "webdriverio": "^4.8.0"
